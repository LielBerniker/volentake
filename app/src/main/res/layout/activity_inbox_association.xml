<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InboxAssociation">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycleRequests"
        android:layout_width="407dp"
        android:layout_height="677dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/backtoassocuserfrominbox"
        tools:layout_editor_absoluteX="4dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/backtoassocuserfrominbox"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp"
        android:backgroundTint="@color/blue"
        android:fontFamily="@font/oswald_font_wght"
        android:text="back"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintBottom_toTopOf="@+id/recycleRequests"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>