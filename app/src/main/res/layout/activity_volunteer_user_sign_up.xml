<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VolunteerUserSignUp">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">


            <EditText
                android:id="@+id/detailName"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:layout_marginTop="48dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:fontFamily="@font/oswald_font_wght"
                android:inputType="textPersonName"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtTitleUpdatePost" />

            <Button
                android:id="@+id/btnPageRequst"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:layout_marginTop="40dp"
                android:backgroundTint="#268BBC"
                android:drawableLeft="@drawable/ic_signup_2"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/CreateUser"
                android:textColor="#FFFFFF"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/inputPassword2"
                tools:ignore="MissingConstraints" />

            <Button
                android:id="@+id/btnBackToFeedOfPosts"
                android:layout_width="100dp"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="40dp"
                android:backgroundTint="#268BBC"
                android:drawableLeft="@drawable/ic_back_1"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/backToMainMenu"
                android:textColor="#FFFFFF"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btnPageRequst"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/txtUpdateNamePost"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="60dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/firstNameText"
                android:textSize="14sp"
                app:layout_constraintEnd_toStartOf="@+id/detailName"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtTitleUpdatePost" />

            <TextView
                android:id="@+id/txtTitleUpdatePost"
                android:layout_width="230dp"
                android:layout_height="45dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/createVolunteerTXT"
                android:textAlignment="center"
                android:textColor="@color/dark_blue"
                android:textSize="24sp"
                android:textStyle="bold|italic"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.497"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/txtUpdateLocationPost"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="30dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/lastNameText"
                android:textSize="14sp"
                app:layout_constraintEnd_toStartOf="@+id/inputLastName"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtUpdateNamePost" />

            <TextView
                android:id="@+id/cityInsertVol1"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="40dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/cityText"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/addressTextCreateVol1" />

            <TextView
                android:id="@+id/streetInsertVol"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="30dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/streetText"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cityInsertVol1" />

            <TextView
                android:id="@+id/textView"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="30dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/numberText"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/streetInsertVol" />

            <TextView
                android:id="@+id/txtPassword1"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="30dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/passwordText"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/emailtxt3" />

            <TextView
                android:id="@+id/txtPassword2"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="30dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/confirmPasswordTXT"
                android:textSize="14sp"
                app:layout_constraintEnd_toStartOf="@+id/inputPassword2"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtPassword1" />

            <TextView
                android:id="@+id/emailtxt3"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="30dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/emailText"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/phoneNumberVol3" />

            <EditText
                android:id="@+id/inputLastName"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="40dp"
                android:fontFamily="@font/oswald_font_wght"
                android:ems="10"
                android:inputType="textPersonName"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/detailName" />

            <EditText
                android:id="@+id/inputcity"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:layout_marginTop="85dp"
                android:layout_marginEnd="50dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:fontFamily="@font/oswald_font_wght"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/birthdateselector" />

            <EditText
                android:id="@+id/insertStreet2"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="50dp"
                android:ems="10"
                android:fontFamily="@font/oswald_font_wght"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/inputcity" />

            <EditText
                android:id="@+id/inputMail"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:inputType="textEmailAddress"
                android:textColor="@color/black"
                android:fontFamily="@font/oswald_font_wght"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/inputPhoneVolSU" />

            <EditText
                android:id="@+id/inputPassword1"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:inputType="textPassword"
                android:textColor="@color/black"
                android:fontFamily="@font/oswald_font_wght"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/inputMail" />

            <EditText
                android:id="@+id/inputPassword2"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="40dp"
                android:fontFamily="@font/oswald_font_wght"
                android:ems="10"
                android:inputType="textPassword"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/inputPassword1" />

            <EditText
                android:id="@+id/insertHouseNumber2"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:layout_marginTop="15dp"
                android:layout_marginEnd="50dp"
                android:ems="10"
                android:inputType="number"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/insertStreet2" />

            <TextView
                android:id="@+id/addressTextCreateVol1"
                android:layout_width="69dp"
                android:layout_height="19dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="40dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/addressText"
                android:textStyle="bold"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/birthdatetxt3" />

            <TextView
                android:id="@+id/birthdateselector"
                android:layout_width="200dp"
                android:layout_height="30dp"
                android:layout_marginTop="30dp"
                android:layout_marginEnd="40dp"
                android:text="Select Birth Date"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:fontFamily="@font/oswald_font_wght"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/inputLastName" />

            <TextView
                android:id="@+id/birthdatetxt3"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="30dp"
                android:text="birth date"
                android:fontFamily="@font/oswald_font_wght"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtUpdateLocationPost" />

            <TextView
                android:id="@+id/phoneNumberVol3"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="56dp"
                android:text="@string/phoneText"
                android:fontFamily="@font/oswald_font_wght"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView" />

            <EditText
                android:id="@+id/inputPhoneVolSU"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:fontFamily="@font/oswald_font_wght"
                android:inputType="phone"
                android:textColor="@color/black"
                android:textSize="18sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/insertHouseNumber2" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>