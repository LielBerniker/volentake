<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout30"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VolunteerUserSignUp">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">


            <Button
                android:id="@+id/btnPageRequest"
                android:layout_width="150dp"
                android:layout_height="50dp"
                android:layout_marginTop="8dp"
                android:backgroundTint="#268BBC"
                android:fontFamily="@font/oswald_font_wght"
                android:text="ADMIT REQUEST"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.462"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btnBackToFeedOfPosts"
                tools:ignore="MissingConstraints" />

            <Button
                android:id="@+id/btnBackToFeedOfPosts"
                android:layout_width="150dp"
                android:layout_height="50dp"
                android:layout_marginTop="68dp"
                android:layout_marginBottom="60dp"
                android:backgroundTint="#268BBC"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/backToMainMenu"
                android:textColor="#FFFFFF"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.462"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btnPageRequst"
                app:layout_constraintVertical_bias="0.0"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/txtUpdateNamePost"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="Name Association"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.124"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/PostImage2" />

            <TextView
                android:id="@+id/detailPostName"
                android:layout_width="194dp"
                android:layout_height="36dp"
                android:layout_marginTop="24dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="Name Association"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.817"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/PostImage2" />

            <TextView
                android:id="@+id/detailLocationPost"
                android:layout_width="194dp"
                android:layout_height="36dp"
                android:layout_marginTop="24dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="location"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.821"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/detailPostName" />

            <TextView
                android:id="@+id/detailNumVolPost"
                android:layout_width="194dp"
                android:layout_height="36dp"
                android:layout_marginTop="12dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="numVol"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.817"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/detailLocationPost" />

            <TextView
                android:id="@+id/detailTypePost"
                android:layout_width="194dp"
                android:layout_height="36dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="type"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.821"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/detailNumVolPost" />

            <TextView
                android:id="@+id/detailPhoneNumberPost"
                android:layout_width="194dp"
                android:layout_height="36dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="Phone Number"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.817"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/detailTypePost" />

            <TextView
                android:id="@+id/detailDescriptionPost"
                android:layout_width="193dp"
                android:layout_height="154dp"
                android:layout_marginTop="72dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="description"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.821"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/detailTypePost" />

            <TextView
                android:id="@+id/txtTitleUpdatePost"
                android:layout_width="232dp"
                android:layout_height="48dp"
                android:layout_marginTop="16dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="Volunteering details"
                android:textAlignment="center"
                android:textColor="#268BBC"
                android:textSize="24sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/txtUpdateLocationPost"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="24dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="Location"
                android:textSize="14sp"
                app:layout_constraintEnd_toStartOf="@+id/detailLocationPost"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtUpdateNamePost" />

            <TextView
                android:id="@+id/txtUpdatePostNumParticipants"
                android:layout_width="109dp"
                android:layout_height="31dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="Num Of Participants"
                android:textSize="14sp"
                app:layout_constraintEnd_toStartOf="@+id/detailNumVolPost"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtUpdateLocationPost" />

            <TextView
                android:id="@+id/txtUpdateTypePost"
                android:layout_width="109dp"
                android:layout_height="31dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="28dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="type"
                android:textSize="14sp"
                app:layout_constraintEnd_toStartOf="@+id/detailTypePost"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtUpdatePostNumParticipants" />

            <TextView
                android:id="@+id/txtUpdatePhonePost"
                android:layout_width="109dp"
                android:layout_height="31dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="Phone Number"
                android:textSize="14sp"
                app:layout_constraintEnd_toStartOf="@+id/detailPhoneNumberPost"
                app:layout_constraintHorizontal_bias="0.018"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtUpdateTypePost" />

            <TextView
                android:id="@+id/txtUpdateDescriptionPost"
                android:layout_width="109dp"
                android:layout_height="31dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="24dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="Description"
                android:textSize="14sp"
                app:layout_constraintEnd_toStartOf="@+id/detailDescriptionPost"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtUpdatePhonePost" />

            <ImageView
                android:id="@+id/PostImage2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="12dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtTitleUpdatePost"
                tools:srcCompat="@tools:sample/avatars" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>