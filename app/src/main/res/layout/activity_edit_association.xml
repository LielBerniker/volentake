<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout8"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VolunteerUserSignUp">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">


            <TextView
                android:id="@+id/txtTitleUpdatePost"
                android:layout_width="300dp"
                android:layout_height="45dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="Edit Association Details"
                android:textAlignment="center"
                android:textColor="@color/dark_blue"
                android:textSize="24sp"
                android:textStyle="bold|italic"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.539"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/txtName"
                android:layout_width="130dp"
                android:layout_height="30dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="56dp"
                android:text="@string/associationNameTXT"
                android:textSize="14sp"
                android:fontFamily="@font/oswald_font_wght"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtTitleUpdatePost" />

            <TextView
                android:id="@+id/txtPhone"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="56dp"
                android:text="@string/phoneText"
                android:textSize="14sp"
                android:fontFamily="@font/oswald_font_wght"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/streetnumtxt2" />

            <TextView
                android:id="@+id/txtAbout"
                android:layout_width="130dp"
                android:layout_height="30dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="34dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/aboutAssociation"
                android:textSize="14sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtPhone" />

            <TextView
                android:id="@+id/txtPassW1"
                android:layout_width="130dp"
                android:layout_height="30dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="56dp"
                android:text="@string/passwordText"
                android:textSize="14sp"
                android:fontFamily="@font/oswald_font_wght"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/associnfoinsert" />

            <TextView
                android:id="@+id/txtPassW2"
                android:layout_width="130dp"
                android:layout_height="30dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="30dp"
                android:text="@string/confirmPasswordTXT"
                android:textSize="14sp"
                android:fontFamily="@font/oswald_font_wght"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/inputPassW1" />

            <TextView
                android:id="@+id/cityAssociationInputTXT1"
                android:layout_width="130dp"
                android:layout_height="30dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="30dp"
                android:text="@string/addressText"
                android:textSize="14sp"
                android:textStyle="bold"
                android:fontFamily="@font/oswald_font_wght"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtName" />

            <EditText
                android:id="@+id/inputName"
                android:layout_width="180dp"
                android:layout_height="45dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:fontFamily="@font/oswald_font_wght"
                android:inputType="textPersonName"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtTitleUpdatePost" />

            <EditText
                android:id="@+id/cityinsert2"
                android:layout_width="180dp"
                android:layout_height="45dp"
                android:layout_marginTop="85dp"
                android:layout_marginEnd="50dp"
                android:ems="10"
                android:fontFamily="@font/oswald_font_wght"
                android:inputType="textPersonName"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/inputName" />

            <EditText
                android:id="@+id/inputPhone"
                android:layout_width="180dp"
                android:layout_height="45dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:fontFamily="@font/oswald_font_wght"
                android:inputType="phone"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/streetnuminsert2" />

            <EditText
                android:id="@+id/associnfoinsert"
                android:layout_width="180dp"
                android:layout_height="125dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:fontFamily="@font/oswald_font_wght"
                android:gravity="start|top"
                android:inputType="textMultiLine"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/inputPhone" />

            <EditText
                android:id="@+id/inputPassW1"
                android:layout_width="180dp"
                android:layout_height="45dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:fontFamily="@font/oswald_font_wght"
                android:inputType="textPassword"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/associnfoinsert" />

            <EditText
                android:id="@+id/inputPassW2"
                android:layout_width="180dp"
                android:layout_height="45dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="40dp"
                android:ems="10"
                android:fontFamily="@font/oswald_font_wght"
                android:inputType="textPassword"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/inputPassW1" />

            <Button
                android:id="@+id/btnaddchanges2"
                android:layout_width="wrap_content"
                android:layout_height="45dp"
                android:layout_marginTop="40dp"
                android:layout_marginBottom="40dp"
                android:backgroundTint="#268BBC"
                android:drawableLeft="@drawable/ic_update_1"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/updateText"
                android:textAlignment="center"
                android:textColor="#FFFFFF"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/inputPassW2" />

            <TextView
                android:id="@+id/citytxt2"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="40dp"
                android:text="@string/cityText"
                android:textSize="14sp"
                android:fontFamily="@font/oswald_font_wght"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cityAssociationInputTXT1" />

            <TextView
                android:id="@+id/streettxt2"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="32dp"
                android:text="@string/streetText"
                android:textSize="14sp"
                android:fontFamily="@font/oswald_font_wght"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/citytxt2" />

            <TextView
                android:id="@+id/streetnumtxt2"
                android:layout_width="110dp"
                android:layout_height="30dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="32dp"
                android:text="@string/numberText"
                android:textSize="14sp"
                android:fontFamily="@font/oswald_font_wght"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/streettxt2" />

            <EditText
                android:id="@+id/streetinsert2"
                android:layout_width="180dp"
                android:layout_height="45dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="50dp"
                android:ems="10"
                android:fontFamily="@font/oswald_font_wght"
                android:inputType="textPersonName"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cityinsert2" />

            <EditText
                android:id="@+id/streetnuminsert2"
                android:layout_width="180dp"
                android:layout_height="45dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="50dp"
                android:ems="10"
                android:fontFamily="@font/oswald_font_wght"
                android:inputType="number"
                android:textSize="14sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/streetinsert2" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>