<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout6"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VolunteerUserSignUp">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical">

            <FrameLayout
                android:id="@+id/frameLayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageView
                    android:layout_width="420dp"
                    android:layout_height="200dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/page_back_01"
                    tools:visibility="visible" />
            </FrameLayout>


            <Button
                android:id="@+id/btnLogOut"
                android:layout_width="150dp"
                android:layout_height="45dp"
                android:layout_marginTop="20dp"
                android:backgroundTint="#268BBC"
                android:text="logout"
                android:textColor="#FFFFFF"
                android:textSize="14sp"
                android:fontFamily="@font/oswald_font_wght"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btnEditVol"
                tools:ignore="MissingConstraints" />

            <Button
                android:id="@+id/btnEditVol"
                android:layout_width="150dp"
                android:layout_height="45dp"
                android:layout_marginTop="20dp"
                android:backgroundTint="#268BBC"
                android:text="edit detailes"
                android:textColor="#FFFFFF"
                android:textSize="14sp"
                android:fontFamily="@font/oswald_font_wght"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.511"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btnaddpic"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/firstnameinsert"
                android:layout_width="130dp"
                android:layout_height="30dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                android:textAlignment="viewStart"
                android:textSize="16sp"
                android:textStyle="bold"
                android:fontFamily="@font/oswald_font_wght"
                app:layout_constraintStart_toEndOf="@+id/imagevolprofile"
                app:layout_constraintTop_toBottomOf="@+id/voltitle" />

            <TextView
                android:id="@+id/emailinsert"
                android:layout_width="150dp"
                android:layout_height="30dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:textAlignment="viewStart"
                android:textSize="14sp"
                android:fontFamily="@font/oswald_font_wght"
                app:layout_constraintStart_toEndOf="@+id/imageView6"
                app:layout_constraintTop_toBottomOf="@+id/firstnameinsert" />

            <TextView
                android:id="@+id/phonenumberinsert"
                android:layout_width="150dp"
                android:layout_height="30dp"
                android:layout_marginStart="10dp"
                android:layout_marginTop="5dp"
                android:textAlignment="viewStart"
                android:textSize="14sp"
                android:fontFamily="@font/oswald_font_wght"
                app:layout_constraintStart_toEndOf="@+id/imageView7"
                app:layout_constraintTop_toBottomOf="@+id/emailinsert" />

            <TextView
                android:id="@+id/voltitle"
                android:layout_width="200dp"
                android:layout_height="45dp"
                android:layout_marginTop="20dp"
                android:fontFamily="@font/oswald_font_wght"
                android:text="@string/VolunteerPageTXT"
                android:textAlignment="center"
                android:textSize="20sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.54"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <Button
                android:id="@+id/btnaddpic"
                android:layout_width="150dp"
                android:layout_height="45dp"
                android:layout_marginTop="50dp"
                android:backgroundTint="#268BBC"
                android:text="add picture"
                android:textColor="#FFFFFF"
                android:fontFamily="@font/oswald_font_wght"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/frameLayout" />

            <ImageView
                android:id="@+id/imagevolprofile"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_marginStart="40dp"
                android:layout_marginTop="15dp"
                android:background="@drawable/button_background"
                app:layout_constraintStart_toStartOf="@+id/frameLayout"
                app:layout_constraintTop_toBottomOf="@+id/voltitle"
                tools:srcCompat="@tools:sample/avatars" />

            <ImageView
                android:id="@+id/imageView6"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                app:layout_constraintStart_toEndOf="@+id/imagevolprofile"
                app:layout_constraintTop_toBottomOf="@+id/firstnameinsert"
                app:srcCompat="@drawable/icon_03" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="15dp"
                app:layout_constraintStart_toEndOf="@+id/imagevolprofile"
                app:layout_constraintTop_toBottomOf="@+id/imageView6"
                app:srcCompat="@drawable/icon_04" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>